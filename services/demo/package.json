{
    "private": true,
    "name": "@react-declarative-form/demo",
    "version": "1.0.17-alpha.1",
    "description": "Interactive demo for react-declarative-form",
    "repository": "valendres/react-declarative-form",
    "author": {
        "name": "Peter Weller",
        "email": "contact@peterweller.com.au",
        "url": "https://github.com/valendres"
    },
    "license": "MIT",
    "scripts": {
        "build": "webpack --config=webpack.config.js",
        "clean": "rimraf dist",
        "compile": "tsc --project ./tsconfig.json --noEmit",
        "format": "prettier \"src/**/*.ts?(x)\"",
        "prebuild": "yarn clean",
        "precommit": "lint-staged",
        "lint": "tslint -c ./tslint.json ./src/**/*.ts --format stylish",
        "start": "webpack serve"
    },
    "lint-staged": {
        "*.{ts,tsx,js,jsx}": [
            "lint --fix",
            "format --write"
        ]
    },
    "resolutions": {
        "scheduler": "0.15.0"
    },
    "dependencies": {
        "@material-ui/core": "4.10.1",
        "@react-declarative-form/core": "^2.0.0",
        "@react-declarative-form/material-ui": "^1.0.0",
        "query-string": "6.13.0",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-hot-loader": "4.12.20",
        "react-json-view": "1.19.1",
        "react-router-dom": "5.2.0",
        "scheduler": "0.15.0",
        "shallow-equal-object": "1.1.1",
        "typeface-roboto": "0.0.75",
        "use-query-params": "1.1.3",
        "validator": "13.0.0",
        "zxcvbn": "4.4.2"
    },
    "devDependencies": {
        "@types/react-router-dom": "5.1.8",
        "@types/zxcvbn": "4.4.1",
        "autoprefixer": "10.3.1",
        "css-loader": "6.2.0",
        "file-loader": "6.2.0",
        "html-webpack-plugin": "5.3.2",
        "postcss-loader": "6.1.1",
        "source-map-loader": "3.0.0",
        "ts-loader": "9.2.5",
        "webpack": "5.48.0",
        "webpack-cli": "4.7.2",
        "webpack-dev-server": "3.11.2",
        "webpack-hot-middleware": "2.25.0"
    }
}
