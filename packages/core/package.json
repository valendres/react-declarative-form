{
    "private": false,
    "name": "@react-declarative-form/core",
    "version": "2.1.0-alpha.8",
    "description": "Declarative react form validator",
    "main": "./dist/index.js",
    "typings": "./dist/index.js",
    "repository": "valendres/react-declarative-form",
    "author": {
        "name": "Peter Weller",
        "email": "contact@peterweller.com.au",
        "url": "https://github.com/valendres"
    },
    "license": "MIT",
    "scripts": {
        "build": "tsc --sourceMap false",
        "clean": "rimraf dist",
        "gen:toc": "markdown-toc -i --maxdepth 5 README.md",
        "lint": "tslint -c ./tslint.json ./src/**/*.ts --format stylish",
        "package": "cd ./dist && yarn pack",
        "prebuild": "yarn clean",
        "precommit": "yarn test --coverage=0 && lint-staged",
        "prepackage": "yarn build",
        "prepublish": "yarn build",
        "prettier": "prettier --write \"?(src|test)/**/*.ts?(x)\"",
        "test": "jest --silent",
        "transpile": "tsc --project ./tsconfig.json --noEmit",
        "verify": "yarn lint && yarn transpile && yarn test"
    },
    "lint-staged": {
        "*.{ts,tsx,js,jsx}": [
            "tslint --fix",
            "prettier --write",
            "git add"
        ]
    },
    "devDependencies": {
        "@types/lodash.omit": "4.5.6",
        "@types/lodash.pick": "4.4.6",
        "@types/validator": "10.11.0",
        "delay": "4.2.0",
        "jest-teamcity-reporter": "^0.9.0",
        "markdown-toc": "1.2.0"
    },
    "peerDependencies": {
        "react": ">=0.16.3",
        "react-dom": ">=16.3.2"
    },
    "dependencies": {
        "fast-deep-equal": "2.0.1",
        "hoist-non-react-statics": "3.3.0",
        "lodash.omit": "4.5.0",
        "lodash.pick": "4.4.0",
        "shallow-equal-object": "1.1.1",
        "update-immutable": "1.5.0",
        "validator": "10.11.0"
    },
    "jest": {
        "setupFilesAfterEnv": [
            "<rootDir>/test/setup.ts"
        ],
        "transform": {
            ".(ts|tsx)": "ts-jest"
        },
        "testRegex": "(<rootDir>/src/.*|(\\.|/)(test))\\.tsx?$",
        "moduleFileExtensions": [
            "js",
            "jsx",
            "ts",
            "tsx"
        ],
        "moduleNameMapper": {
            "^@components(.*)": "<rootDir>/src/components$1",
            "^@rules(.*)": "<rootDir>/src/rules$1",
            "^@types(.*)": "<rootDir>/src/types$1",
            "^@utils(.*)": "<rootDir>/src/utils$1",
            "^@validator(.*)": "<rootDir>/src/validator$1",
            "\\.(s?css|less)$": "<rootDir>/test/style-mock.ts"
        },
        "collectCoverage": true,
        "collectCoverageFrom": [
            "!src/typings/**/*.ts",
            "!src/**/*.test.{ts,tsx}",
            "!src/**/*.snap",
            "src/**/*.{ts,tsx}"
        ],
        "coverageReporters": [
            "html",
            "lcov"
        ],
        "coverageDirectory": "./coverage",
        "modulePathIgnorePatterns": [
            "<rootDir>/coverage/"
        ],
        "testResultsProcessor": "jest-teamcity-reporter"
    },
    "gitHead": "f4123eb4383492943c25229283c7fc4cf054c29d"
}
